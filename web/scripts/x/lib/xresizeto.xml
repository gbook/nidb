<x_symbol id='xResizeTo'>

<sources>
  <src><file>xresizeto.js</file><note><![CDATA[]]></note></src>
</sources>

<groups>
  <grp>Size</grp>
</groups>
<type>Function</type>

<description><![CDATA[Resize an element.]]></description>

<syntax>xResizeTo(e, uW, uH)</syntax>

<parameters>
  <par>
    <name>e</name>
    <note><![CDATA[id string or element reference]]></note>
  </par>
  <par>
    <name>uW</name>
    <note><![CDATA[unsigned integer]]></note>
  </par>
  <par>
    <name>uH</name>
    <note><![CDATA[unsigned integer]]></note>
  </par>
</parameters>

<return><![CDATA[Returns an object with 'w' and 'h' properties giving the current width and height of the element.]]></return>

<dependencies>
  <dep>xwidth</dep>
  <dep>xheight</dep>
</dependencies>

<demos>
