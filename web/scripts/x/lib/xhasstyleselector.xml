<x_symbol id='xHasStyleSelector'>

<sources>
  <src><file>xhasstyleselector.js</file><note><![CDATA[]]></note></src>
</sources>

<groups>
  <grp>Style</grp>
</groups>

<type>Function</type>

<description><![CDATA[Checks whether any of the stylesheets attached to the document contain the definition of the specified style selector (simple string matching at the moment).]]></description>

<syntax>xHasStyleSelector(styleSelectorString)</syntax>

<parameters>
  <par><name>styleSelectorString</name><note><![CDATA[]]></note></par>
</parameters>

<return><![CDATA[undefined  - style sheet scripting not supported by the browser<br>true/false - found/not found]]></return>

<dependencies>
  <dep>xHasStyleSheets</dep>
  <dep>xTraverseDocumentStyleSheets</dep>
</dependencies>

