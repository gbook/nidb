<x_symbol id='xStrReplaceEnd'>

<sources>
  <src><file>xstrreplaceend.js</file><note><![CDATA[]]></note></src>
</sources>

<groups>
  <grp>String</grp>
</groups>

<type>Function</type>

<description><![CDATA[Replaces newEnd.length characters at the end of s with newEnd.]]></description>

<syntax>xStrReplaceEnd( s, newEnd )</syntax>

<parameters>
  <par><name>s</name><note><![CDATA[]]></note></par>
  <par><name>newEnd</name><note><![CDATA[]]></note></par>
</parameters>

<return><![CDATA[Returns the new string. If newEnd.length > s.length then newEnd is returned.]]></return>

<dependencies>
  <dep>xStr</dep>
</dependencies>

