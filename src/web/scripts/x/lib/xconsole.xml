<x_symbol id='xConsole'>

<sources>
  <src><file>xconsole.js</file><note><![CDATA[]]></note></src>
</sources>

<groups>
  <grp>Debug</grp>
</groups>

<type>Var</type>

<description><![CDATA[A debugging utility.]]></description>

<syntax>xConsole.log(msg, win)</syntax>

<methods>
  <meth><name>log</name><note><![CDATA[Passes msg to win.console.log or win.opera.postError or appends it to the innerHTML of element with id "xConsoleElement" or assigns it to win.status. Uses window if win is undefined.]]></note></meth>
</methods>

